; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "AnimationTree",
"class": "UnitAnimator",
"language": "GDScript",
"path": "res://scripts/units/UnitAnimator.gd"
}, {
"base": "Reference",
"class": "UnitData",
"language": "GDScript",
"path": "res://scripts/units/UnitData.gd"
}, {
"base": "Spatial",
"class": "UnitObject",
"language": "GDScript",
"path": "res://scripts/units/UnitObject.gd"
} ]
_global_script_class_icons={
"UnitAnimator": "",
"UnitData": "",
"UnitObject": ""
}

[application]

config/name="Strategy Steam Punk"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://scripts/Globals.gd"
GlobalBus="*res://scripts/GlobalBus.gd"
DebugDraw="*res://addons/zylann.debug_draw/debug_draw.gd"
DrawLine3d="*res://addons/Godot-3D-Lines/DrawLine3D.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/blend/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
